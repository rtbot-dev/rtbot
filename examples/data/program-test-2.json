{
    "title": "Peak detector",
    "description": "This is a program to detect peaks",
    "date": "now",
    "version": "v1",
    "author": "Eduardo",
    "license": "MIT",
    "entryNode": "ma1",
    "optim": {
        "algorithm": "Nelder-Mead"
    },
    "operators": [
        {
            "id": "in1",
            "type": "Input"
        },
        {
            "id": "peak",
            "type": "PeakDetector",
            "n": 3
        },
        {
            "id": "join",
            "type": "Join",
            "numPorts": 2
        }
    ],
    "connections": [
        {
            "from": "in1",
            "to": "peak",
            "fromPort": "o1",
            "toPort": "i1"
        },
        {
            "from": "peak",
            "to": "join",
            "fromPort": "o1",
            "toPort": "i1"
        },
        {
            "from": "in1",
            "to": "join",
            "fromPort": "o1",
            "toPort": "i2"
        }
    ]
}